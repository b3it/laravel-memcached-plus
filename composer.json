{
  "name": "b3it/laravel-memcached-plus",
  "description": "Extends the built-in Laravel 5 Cache Memcached driver adding support for persistent connections, SASL and Memcached options.",
  "keywords": ["laravel", "memcached", "sasl", "persistent", "options", "memcachier", "memcachedcloud", "heroku", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Castleman",
      "email": "tom@b3it.co"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ext-memcached": "*",
    "illuminate/cache": "5.*",
    "illuminate/session": "5.*"
  },
  "require-dev": {
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "^4.8|^5.1"
  },
  "autoload": {
    "psr-4": {
      "B3IT\\MemcachedPlus\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "4.0.x-dev"
    }
  }
}
